Date: WOTextField {
	size      = 20;
    formatter = session.timeFormatter;
	value     = objectPropertyValue;
	id        = name;
	readonly  = 1;
}

DateSelector: WOImage {
	filename  = "icons/cal.gif";
	framework = "Grader";
	width     = 16;
	height    = 16;
	alt       = "Select date";
	title     = "Select date";
	id        = "~\"select\" + name";
}

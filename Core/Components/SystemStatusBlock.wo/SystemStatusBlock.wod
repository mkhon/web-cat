SubsystemRows: net.sf.webcat.core.SubsystemFragmentCollector {
    index       = index;
    fragmentKey = "systemStatusRows";
}

StartTime: WOString {
    formatter = session.timeFormatter;
	value     = application.startTime;
}

TR : net.sf.webcat.core.TableRow {
    index     = index;
    increment = true;
}

UserCount: WOString {
	value = userCount;
}

IncludeSeparator: WOConditional {
    condition = includeSeparator;
}

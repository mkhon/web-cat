Icons : WORepetition {
    list = icons;
    item = iconInRepetition;
}

Link : WOHyperlink {
    action = performAction;
    title = iconInRepetition.title;
    onclick = "~jsWrapperID + '.selectOnlyObjectAtIndexInBatch(' + indexInBatch + ', { synchronous: true });'";
}

Icon : WOImage {
    framework = iconInRepetition.framework;
    filename = iconInRepetition.filename;
}

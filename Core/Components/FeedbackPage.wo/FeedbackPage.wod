Category: WOPopUpButton {
	list = categories;
	selection = selectedCategory;
}

Form1: WOForm {
    multipleSubmit = true;
}

Page: net.sf.webcat.core.WCPageWithNavigation {
}

NotSent: WOConditional {
	condition = sent;
	negate = true;
}

Sent: WOConditional {
	condition = sent;
}

SubmitButton: WOSubmitButton {
	action = sendFeedback;
	value  = "Send Comment";
}

Text: WOText {
	rows  = 18;
	cols  = 80;
	value = comments;
}

UserName: WOString {
	value = user.nameAndUid;
}

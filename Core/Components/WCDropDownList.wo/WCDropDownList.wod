Control: WOGenericContainer {
    elementName = "span";
    class = "WCDropDownList";
    id = id;
}

DropDown: WCContentPane {
    id = menuId;
    class = "WCDropDownListMenu";
}

DropDownList: WOGenericContainer {
    elementName = "ul";
    style = cssListStyle;
}

Item: WCDropDownItem {
    isSelectable = true;
}

Selection: WOString {
    value = displayStringForSelection;
}

SelectionSpan: WOGenericContainer {
    elementName = "span";
    id = selectionId;
}

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="net.sf.webcat.oda.core.emptyAppContextHandlers">
      <resultProvider
            class="net.sf.webcat.oda.designer.preview.PreviewingResultSetProvider">
      </resultProvider>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            name="%profile.propertypage.name"
            class="org.eclipse.datatools.connectivity.oda.design.ui.pages.impl.DefaultDataSourcePropertyPage"
            id="%oda.data.source.id">
          <filter
               name="org.eclipse.datatools.profile.property.id"
               value="%oda.data.source.id">
          </filter>
          <enabledWhen>
              <instanceof
                  value="org.eclipse.datatools.connectivity.IConnectionProfile">
              </instanceof>
          </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.connectionProfile">
      <newWizard
            name="%newwizard.name"
            icon="icons/new_oda_dswiz.ico"
            profile="%oda.data.source.id"
            description="%newwizard.description"
            class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.NewDataSourceWizard"
            id="%oda.data.source.id">
      </newWizard>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.oda.design.ui.dataSource">
      <dataSourceUI
            id="%oda.data.source.id">
         <newDataSourceWizard
               pageClass="org.eclipse.datatools.connectivity.oda.design.ui.pages.impl.DefaultDataSourceWizardPage"
               includesProgressMonitor="false"
               pageTitle="%wizard.data.source.page.title"
               windowTitle="%wizard.window.title">
         </newDataSourceWizard>
      </dataSourceUI>
      <dataSetUI
            supportsOutParameters="false"
            supportsInParameters="true"
            initialPageId="net.sf.webcat.oda.dataSet.columnMappingPage"
            id="net.sf.webcat.oda.dataSet">
         <dataSetWizard
               class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.DataSetWizard"
               windowTitle="%wizard.data.set.window.title">
         </dataSetWizard>
         <dataSetPage
               displayName="%wizard.data.set.columnMappingPage.title"
               icon="icons/datasetpage.ico"
               wizardPageClass="net.sf.webcat.oda.designer.impl.ColumnMappingPage"
               path="/"
               id="net.sf.webcat.oda.dataSet.columnMappingPage">
         </dataSetPage>
      </dataSetUI>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.datatools.connectivity.oda.design.internal.ui.preferences.OpenDataAccessPage"
            class="net.sf.webcat.oda.designer.preferences.DataSetPreferencePage"
            id="net.sf.webcat.oda.ui.preferences.DataSetPreferencePage"
            name="%preferences.data.set">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="%command.category.description"
            id="net.sf.webcat.oda.ui.commands.webCatCategory"
            name="%command.category.name">
      </category>
      <command
            categoryId="net.sf.webcat.oda.ui.commands.webCatCategory"
            id="net.sf.webcat.oda.ui.commands.resetPreviewCache"
            name="%command.resetPreviewCache.name">
      </command>
      <command
            categoryId="net.sf.webcat.oda.ui.commands.webCatCategory"
            id="net.sf.webcat.oda.ui.commands.openWebCat"
            name="%command.openWebCat.name">
      </command>
      <command
            categoryId="net.sf.webcat.oda.ui.commands.webCatCategory"
            id="net.sf.webcat.oda.ui.commands.refreshContentAssist"
            name="%command.refreshContentAssist.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="net.sf.webcat.oda.ui.menus.webCat"
               label="%menu.webCat.name"
               mnemonic="%menu.webCat.mnemonic">
            <command
                  commandId="net.sf.webcat.oda.ui.commands.openWebCat"
                  id="net.sf.webcat.oda.ui.menus.openWebCat"
                  mnemonic="%menu.openWebCat.mnemonic">
            </command>
            <separator
                  name="net.sf.webcat.oda.ui.menus.openWebCatGroupEnd"
                  visible="true">
            </separator>
            <command
                  commandId="net.sf.webcat.oda.ui.commands.resetPreviewCache"
                  id="net.sf.webcat.oda.ui.menus.resetPreviewCache"
                  mnemonic="%menu.resetPreviewCache.mnemonic">
            </command>
            <command
                  commandId="net.sf.webcat.oda.ui.commands.refreshContentAssist"
                  id="net.sf.webcat.oda.ui.menus.refreshContentAssist"
                  mnemonic="%menu.refreshContentAssist.mnemonic">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup></startup>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="net.sf.webcat.oda.designer.handlers.ResetPreviewCacheHandler"
            commandId="net.sf.webcat.oda.ui.commands.resetPreviewCache">
      </handler>
      <handler
            class="net.sf.webcat.oda.designer.handlers.OpenWebCATHandler"
            commandId="net.sf.webcat.oda.ui.commands.openWebCat">
      </handler>
      <handler
            class="net.sf.webcat.oda.designer.handlers.RefreshContentAssistHandler"
            commandId="net.sf.webcat.oda.ui.commands.refreshContentAssist">
      </handler>
   </extension>
   <extension
         point="org.eclipse.birt.report.designer.ui.editors.multiPageEditorContributor">
      <targetEditor
            targetEditorId="org.eclipse.birt.report.designer.ui.editors.ReportEditor">
         <formPage
               actionBarContributor="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
               class="net.sf.webcat.oda.designer.metadata.OverviewFormPage"
               displayName="Overview"
               id="net.sf.webcat.oda.ui.editors.metadata"
               position="left"
               relative="org.eclipse.birt.report.designer.ui.editors.layout"
               visible="true">
         </formPage>
      </targetEditor>
   </extension>

</plugin>

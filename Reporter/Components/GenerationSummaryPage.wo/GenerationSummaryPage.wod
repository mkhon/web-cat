Back: WOSubmitButton {
	value = " < Back ";
	action = localPageController.previousPage;
}

RenderingMethod: WOPopUpButton {
	list = renderingMethods;
	item = renderingMethod;
	selection = selectedRenderingMethod;
	displayString = renderingMethod.displayName;  // VALID
}

Next: WOSubmitButton {
	value = " Next > ";
	name  = "NextButton";
	action = goNext;
}

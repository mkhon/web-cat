Page: net.sf.webcat.core.WCPageWithNavigation {
}

ChangeReportTemplate: WCLink {
    action = changeReportTemplate;
}

QueryBuilder : net.sf.webcat.core.objectquery.ObjectQueryBuilder {
    objectType = dataSet.wcEntityName;
    surrogate = objectQuerySurrogateForDataSet;
}

ReportParameter: ReportParameterValueComponent {
    parameter = parameter;
    value = valueForParameter;
}

GenerateReport: WCButton {
    label = "Generate Report";
    action = generateReport;
}

<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: export-plugin.xml,v 1.4 2010/05/11 15:52:44 aallowat Exp $ -->

<project default="plugin_export" name="build" basedir=".">
    <target name="plugin_export">
        <pde.exportPlugins
            destination="../BIRTRuntime/Resources/ReportEngine"
            exportSource="false"
            exportType="directory"
            plugins="org.webcat.oda.core"
            useJARFormat="true"/>
        <eclipse.refreshLocal
            resource="BIRTRuntime/Resources/ReportEngine"
            depth="infinite"/>
    </target>
</project>
